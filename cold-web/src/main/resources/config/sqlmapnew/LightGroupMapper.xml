<?xml version="1.0" encoding="UTF-8"?>
		<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
				"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.manage.cold.dao.newdb.LightGroupMapper">

   <select id="getLightSetByRdcId" parameterType="map" resultType="com.smartcold.manage.cold.entity.comm.ItemObject">
     select * from `smartcold`.`coldstoragelightset` where rdcid=#{rdcId} ORDER BY coldStorageId asc;
    </select>
   <select id="getLightSetByColdStorageId" parameterType="map" resultType="com.smartcold.manage.cold.entity.comm.ItemObject">
      SELECT * from `smartcold`.`coldstoragelightset`  where coldStorageId=#{coldStorageId};
    </select>


  
    
</mapper>