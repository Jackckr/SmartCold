<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.manage.cold.dao.newdb.DevStatusMapper">

	<insert id="addDataList" parameterType="StorageDataCollectionEntity">
		INSERT INTO `devstatus`
		(
		`deviceid`,
		`apid`,
		`key`,
		`value`,
		`time`,
		`addtime`
		)
		VALUES
		<foreach collection="list" item="model" separator=","> 
        (           
            #{model.deviceid}, 
            #{model.apid}, 
            #{model.key}, 
            #{model.value}, 
            #{model.time}, 
            now()     
        )
    </foreach>
	</insert>

</mapper>