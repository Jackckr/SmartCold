<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.manage.cold.dao.newdb.MaintenanceInfoMapper">
 <insert id="addMaintenanceInfo">
	INSERT INTO `maintenance` (`waruser`,`maintuser`,`companyinfo`,`note`,`repairtime`,`bookingtime`,`cost`,`servertype`,`mappid`,`state`,`addtime` ) VALUES(#{waruser},#{maintuser},#{companyinfo},#{note}, #{repairtime},#{bookingtime},#{cost},#{servertype},#{mappid}, #{state},#{addtime});
 </insert>
 

	


</mapper>