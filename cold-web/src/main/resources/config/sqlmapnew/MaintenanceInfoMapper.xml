<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.manage.cold.dao.newdb.MaintenanceInfoMapper">
 <insert id="addMaintenanceInfo" parameterType="MaintenanceInfo" useGeneratedKeys="true" keyProperty="id">
	INSERT INTO `maintenance` (`waruser`,`maintuser`,`companyinfo`,`note`,`repairtime`,`bookingtime`,`cost`,`servertype`,`warmappid`,`state`,`addtime` ) VALUES(#{waruser},#{maintuser},#{companyinfo},#{note}, #{repairtime},#{bookingtime},#{cost},#{servertype},#{warmappid}, #{state},#{addtime});
 </insert>
 
 <delete id="delMaintenanceById" parameterType="map">
   DELETE from `maintenance` where `id`=#{id};
 </delete>
 
 <select id="getMaintenanceById" resultType="MaintenanceInfo">
  select * from `maintenance` where `id`=#{id};
 </select>
 

	


</mapper>