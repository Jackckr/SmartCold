<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.manage.cold.dao.olddb.MessageMapper">
   
    
    <insert id="addsystemInform" parameterType="SystemInformEntity">
        INSERT INTO `systemInform` (`type`,`stype`,`rdcid`,`uid`,`level`,`isread`,`state`,`tit`,`msg`,`addtime`)VALUES(#{type},#{stype},#{rdcid},#{uid},#{level},#{isread},#{state},#{tit},#{msg},now());
    </insert>
    
</mapper>