<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.manage.cold.dao.olddb.WindScreenSetMapping">

    <select id="findById" parameterType="map" resultType="WindScreenSetEntity">
		SELECT *
		FROM `windscreenset` WHERE `id` = #{id}
	</select>
	
	<select id="findByStorageId" parameterType="map" resultType="WindScreenSetEntity">
		SELECT *
		FROM `windscreenset` WHERE `coldstorageid` = #{storageid}
	</select>

</mapper>