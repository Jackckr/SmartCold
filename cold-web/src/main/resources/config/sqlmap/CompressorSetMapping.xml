<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.manage.cold.dao.olddb.CompressorSetMapping">

    <select id="findCompressorByid" parameterType="map" resultType="CompressorSetEntity">
		SELECT *
		FROM `compressorset` WHERE `id` = #{id} ORDER BY `addTime`
	</select>
	
	<select id="findCompressorByGroupid" parameterType="map" resultType="CompressorSetEntity">
		SELECT *
		FROM `compressorset` WHERE `compressorgroupid` = #{groupId} ORDER BY `addTime`
	</select>

</mapper>