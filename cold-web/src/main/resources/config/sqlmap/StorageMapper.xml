<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.manage.cold.dao.olddb.StorageMapper">

	<select id="findStorageById" parameterType="map" resultType="StorageEntity">
		SELECT *
		FROM `Storage` WHERE `id` = #{id}
	</select>
	
	<select id="findStorageByName" parameterType="map" resultType="StorageEntity">
		SELECT *
		FROM `Storage` WHERE `name` = #{name}
	</select>

</mapper>