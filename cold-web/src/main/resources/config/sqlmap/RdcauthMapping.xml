<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.smartcold.manage.cold.dao.olddb.RdcauthMapping">

    <select id="getRdcAuthByUid" parameterType="Integer" resultType="Integer">
      SELECT `id` from `rdcauth` where  `uid`=${userId} and `state`=1;
    </select>
    
    <insert id="insertCertification" parameterType="RdcAuthEntity" useGeneratedKeys="true" keyProperty="id">
		 INSERT INTO `rdcauth` (`uid`, `type`, `rdcid`, `state`, `note` , `imgurl`, `addtime `) VALUES (#{uid}, #{type}, #{rdcid}, #{state}, #{note} , #{imgurl}, now());
    </insert>
    
    
    
</mapper>