<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.manage.cold.dao.olddb.PlatformDoorSetMapping">

    <select id="findById" parameterType="map" resultType="PlatformDoorSetEntity">
		SELECT *
		FROM `platformdoorset` WHERE `id` = #{id}
	</select>
	
	<select id="findByRdcId" parameterType="map" resultType="PlatformDoorSetEntity">
		SELECT *
		FROM `platformdoorset` WHERE `rdcId` = #{rdcId}
	</select>

</mapper>