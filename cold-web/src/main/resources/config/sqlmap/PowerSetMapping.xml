<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.manage.cold.dao.olddb.PowerSetMapping">

    <select id="findById" parameterType="map" resultType="PowerSetEntity">
		SELECT *
		FROM `powerset` WHERE `id` = #{id}
	</select>
	
	<select id="findByRdcId" parameterType="map" resultType="PowerSetEntity">
		SELECT *
		FROM `powerset` WHERE `rdcId` = #{rdcId}
	</select>
	
	<select id="findByFilter" parameterType="map" resultType="PowerSetEntity">
		SELECT * FROM `powerset` where 1=1  
		<if test="oid!=null and oid!=''"> and id in(${oid})</if>  
		<if test="iunbalance!=null"> and iunbalance>${iunbalance}</if> 
	</select>

</mapper>