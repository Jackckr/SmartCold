<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.zigbee.manage.dao.StorageManageTypeMapper">

	<select id="findAll" parameterType="map" resultType="StorageManageTypeEntity">
		SELECT *
		FROM `storagemanagetype` order by `id` ASC
	</select>
	<select id="getTypeById" resultType="java.lang.String">
		SELECT `type` FROM  `storagemanagetype` WHERE `id`=#{id}
	</select>

</mapper>