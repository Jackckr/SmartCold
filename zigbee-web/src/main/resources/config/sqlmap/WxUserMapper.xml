<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.smartcold.zigbee.manage.dao.WxUserMapper" >
    <insert id="insertByMap" parameterType="map">
        INSERT INTO `wxuser` (`nickname`,`openid`,`addtime`) VALUES (#{nickname},#{openid},#{addtime})
    </insert>
    <select id="findByOpenId" resultType="java.util.HashMap">
        SELECT * FROM `wxuser` WHERE `openid`=#{openid}
    </select>
</mapper>