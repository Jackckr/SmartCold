<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.zigbee.manage.dao.StorageTemperTypeMapper">

	<select id="findAll" parameterType="map" resultType="StorageTemperTypeEntity">
		SELECT *
		FROM `storagetempertype` order by `id` ASC
	</select>
    <select id="findTypeById" resultType="java.lang.String">
		SELECT `type` FROM `storagetempertype` WHERE `id`=#{id}
	</select>

</mapper>