<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.zigbee.manage.dao.ProvinceListMapper">

    <select id="findProvinceList" parameterType="map" resultType="ProvinceListEntity">
        SELECT *
        FROM `provincelist` WHERE `ProvinceID` &lt;= 34
    </select>
    <select id="findNameById" resultType="java.lang.String">
        SELECT `ProvinceName` FROM `provincelist` WHERE `ProvinceId`=#{provinceId}
    </select>

</mapper>