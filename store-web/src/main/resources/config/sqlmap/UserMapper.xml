<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.manage.cold.dao.olddb.UserMapper">

	<select id="login" parameterType="map" resultType="com.smartcold.manage.cold.entity.olddb.UserEntity">
		select u.`id`,u.`type`, u.`role`, u.`username`,u.`avatar` from `user` u  where u.username=#{username} and password =#{password};;
	</select>
	
	
</mapper>