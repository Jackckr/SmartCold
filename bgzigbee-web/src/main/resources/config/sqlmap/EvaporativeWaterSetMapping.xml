<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.smartcold.bgzigbee.manage.dao.EvaporativeWaterSetMapping">
    <insert id="insert">
        INSERT INTO `evaporativewaterset` (`evaporativeid`, `name`,`groupid`)
        VALUES (#{evaporativeid}, #{name}, #{groupid})
    </insert>
    <select id="findById" parameterType="map" resultType="com.smartcold.bgzigbee.manage.entity.EvaporativeWaterSetEntity">
        SELECT *
        FROM `evaporativewaterset` WHERE `id` = #{id}
    </select>

    <select id="findByEvaporativeId" parameterType="map" resultType="com.smartcold.bgzigbee.manage.entity.EvaporativeWaterSetEntity">
        SELECT *
        FROM `evaporativewaterset` WHERE `evaporativeid` = #{evaporativeid}
    </select>
</mapper>