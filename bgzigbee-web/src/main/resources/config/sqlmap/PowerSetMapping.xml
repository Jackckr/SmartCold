<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.smartcold.bgzigbee.manage.dao.PowerSetMapping">
    <select id="findById" parameterType="map" resultType="com.smartcold.bgzigbee.manage.entity.PowerSetEntity">
        SELECT *
        FROM `powerset` WHERE `id` = #{id}
    </select>

    <select id="findByRdcId" parameterType="map" resultType="com.smartcold.bgzigbee.manage.entity.PowerSetEntity">
        SELECT *
        FROM `powerset` WHERE `rdcId` = #{rdcId}
    </select>
    
      <delete id="delete" parameterType="map">
        DELETE FROM `powerset` WHERE `id` = #{id}
    </delete>
    
    <update id="update" parameterType="PowerSetEntity">
        UPDATE `powerset`
        SET
            `name` = #{name},
            `iunbalance` = #{iunbalance}
        WHERE `id` = #{id}
    </update>
</mapper>