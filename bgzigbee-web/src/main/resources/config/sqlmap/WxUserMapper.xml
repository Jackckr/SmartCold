<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.smartcold.bgzigbee.manage.dao.WxUserMapper" >

    <select id="findByFilter" resultType="com.smartcold.bgzigbee.manage.entity.WxUserEntity">
        SELECT * FROM `wxuser`
        <where>
            <if test="keyword != null and keyword != ''">`nickname` LIKE  "%"#{keyword}"%"</if>
        </where>
    </select>
</mapper>