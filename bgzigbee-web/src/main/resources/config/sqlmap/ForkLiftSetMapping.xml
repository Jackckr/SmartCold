<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.smartcold.bgzigbee.manage.dao.ForkLiftSetMapping">
    <insert id="insert">
        INSERT INTO `forkliftset` WHERE (`rdcid`, `name`, `power`)
        VALUES (#{rdcid}, #{name}, #{power})
    </insert>

    <select id="findById" parameterType="map" resultType="com.smartcold.bgzigbee.manage.entity.ForkLiftSetEntity">
        SELECT *
        FROM `forkliftset` WHERE `id` = #{id}
    </select>

    <select id="findByRdcId" parameterType="map" resultType="com.smartcold.bgzigbee.manage.entity.ForkLiftSetEntity">
        SELECT *
        FROM `forkliftset` WHERE `rdcId` = #{rdcId}
    </select>
    
     <delete id="delete" parameterType="map">
        DELETE FROM `forkliftset` WHERE `id` = #{id}
    </delete>
    
    <update id="update" parameterType="com.smartcold.bgzigbee.manage.entity.ForkLiftSetEntity">
        UPDATE `forkliftset`
        SET
            `name` = #{name},
            `power` = #{power}
        WHERE `id` = #{id}
    </update>
    
</mapper>