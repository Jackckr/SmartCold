<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.smartcold.bgzigbee.manage.dao.DeviceObjectMappingMapper">
    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO `deviceObjectMapping` (`deviceid`, `oid`, `type`)
        VALUES (#{deviceid}, #{oid}, #{type})
    </insert>
    <select id="findByTypeOid" resultType="com.smartcold.bgzigbee.manage.entity.DeviceObjectMappingEntity">
        SELECT * FROM `deviceObjectMapping`
        WHERE `type`=#{type} and `oid`=#{oid}
    </select>
</mapper>