<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.smartcold.bgzigbee.manage.dao.ColdstorageLightSetMapping">
    <insert id="insert">
        INSERT INTO `coldstoragelightset` (`rdcid`, `name`, `power`)
        VALUES (#{rdcid}, #{name}, #{power})
    </insert>
    <select id="findById" parameterType="map" resultType="com.smartcold.bgzigbee.manage.entity.ColdStorageLightSetEntity">
        SELECT *
        FROM `coldstoragelightset` WHERE `id` = #{id}
    </select>

    <select id="findByRdcId" parameterType="map" resultType="com.smartcold.bgzigbee.manage.entity.ColdStorageLightSetEntity">
        SELECT *
        FROM `coldstoragelightset` WHERE `rdcId` = #{rdcId}
    </select>


</mapper>